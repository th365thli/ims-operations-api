package com.ibm.ims.ea.om.pgm.update.syntax;

/** Specifies the Fast Path option.  */
public class FP implements SET{
	
	private String result = null;
	private static String NAME = "FP";
	
	public enum _PGM_FP{
		/** The program is a Fast Path-exclusive application program.*/
		E,
		
		/** The program is not a Fast Path-exclusive application program.*/
		N;
	}

	public FP(_PGM_FP key) {
    	this.result = key.name();
	}
	
	@Override
	public String toString(){
		StringBuilder strBld = new StringBuilder();
		strBld.append(this.result);
	    return strBld.toString();
	}
	
	@Override
	public String getName() {
		return NAME;
	}
}
