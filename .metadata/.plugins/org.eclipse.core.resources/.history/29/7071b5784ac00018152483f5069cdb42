package com.ibm.ims.ea.om.pgm.update.syntax;

/** Specifies whether transaction level statistics should be logged. */
public class TRANSTAT implements SET{
	
	private String result = null;
	private static String NAME = "TRANSTAT";
	
	public enum _PGM_TRANSTAT{
		/** should not be logged*/
		N,
		
		/** should be logged*/
		Y;
	}

	public TRANSTAT(_PGM_TRANSTAT key) {
    	this.result = key.name();
	}
	
	@Override
	public String toString(){
		StringBuilder strBld = new StringBuilder();
		strBld.append(this.result);
	    return strBld.toString();
	}
	
	@Override
	public String getName() {
		return NAME;
	}
}
