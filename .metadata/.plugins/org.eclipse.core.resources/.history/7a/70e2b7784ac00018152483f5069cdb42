package com.ibm.ims.ea.om.icon.query.rmtimscon;

public interface IconRmtImsConQueryCommand {
	
	public enum SHOW {
		ALL,   
        APPL,        
        AUTOCONN,       
        HOSTNAME,       
        IDLETO,       
        IPADDR,         
        NUMSOC,       
        PERSISTENT,       
        PORT,        
        RESVSOC,        
        SENDCLNT,       
        STATUS,         
        SUMMARY,         
        USERID; 
	}
	
	public enum SCSTATUS {
		CONN,
		RECV;
		
	}
	
	public enum STATUS {
		ACTIVE,
		STOPPED,
		NOTACTIVE;
	}
	
	public void setName(String[] names);
	public void setRoute(String[] routeName);
	public void setSendClnt(String[] sendClnt);
	public void setShow(SHOW[] shows);
	public void setSCStatus(SCSTATUS[] states);
	public void setStatus(STATUS[] status);
	
	
}
