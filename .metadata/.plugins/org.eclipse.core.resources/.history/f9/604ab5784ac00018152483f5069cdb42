package com.ibm.ims.ea.om.pgm.update.syntax;

/** Specifies the generated PSB option.  */
public class GPSB implements SET{
	
	private String result = null;
	private static String NAME = "GPSB";
	
	public enum _PGM_GPSB{
		/** The PSB associated with the program is not generated by IMS. */
		N,
		
		/** The PSB associated with the program is generated by IMS.*/
		Y;
	}

	public GPSB(_PGM_GPSB key) {
    	this.result = key.name();
	}
	
	@Override
	public String toString(){
		StringBuilder strBld = new StringBuilder();
		strBld.append(this.result);
	    return strBld.toString();
	}
	
	@Override
	public String getName() {
		return NAME;
	}
}
