package com.ibm.ims.ea.om.icon.update.client;

public interface IconClientUpdateCommand {
	
	public enum PORT {
		LOCAL;
	}
	
	public enum STOP {
		COMM;
	}
	
	public void setName(String[] names);
	
	public void setPort(PORT port, String[] names);
	
	public void setPort(PORT port);
	
	public void setPort(String[] names);
	
	public void setStop(STOP comm);
	
	public void setRoute(String[] imsName);
}
