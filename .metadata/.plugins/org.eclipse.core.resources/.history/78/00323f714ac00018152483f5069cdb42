package com.ibm.ims.ea.om.icon;

public class ImsconnectOdbm {

    /**
     * Statuses returned from IMSConnect(ODBM) Attribute ASTT
     */
    public static enum STATUS {
        
        //Status from Attribute ASTT:
        ACTIVE("ACTIVE"),
        NOTACTIVE_IMSCON("NOTACTIVE(IMSCON)"),
        NOTACTIVE_ODBM("NOTACTIVE(ODBM)"),      
        NOTACTIVE_IMSCON_ODBM("NOTACTIVE(IMSCON,ODBM)"),
        
        //Status from Attribute STT:
        REGISTERED("REGISTERED"),
        NOTACTIVE("NOTACTIVE");
        
        private String value = "";

        private STATUS(String val){
            this.value = val;
        }

        private STATUS(){}

        @Override
        public String toString(){
            return this.value;
        }
    }
}

