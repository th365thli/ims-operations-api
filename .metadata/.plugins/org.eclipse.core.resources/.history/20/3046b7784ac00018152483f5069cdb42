package com.ibm.ims.ea.om.icon.update;

import com.ibm.ims.ea.om.icon.update.client.IconClientUpdateCommandBuilder;
import com.ibm.ims.ea.om.icon.update.datastore.IconDatastoreUpdateCommandBuilder;

public class IconUpdateCommandBuilder {

	/**
	 * Command types allowed for IMS Connect Type 2 command:
	 *  ALIAS,CLIENT,CONFIG,DATASTORE,IMSPLEX,LINK,MSC,ODBM,PORT,RMTIMSCON,SENDCLNT,UOR       
	 */
	public enum ICON_COMMAND_SUBTYPE {CLIENT, DATASTORE};
	
	
	public IconDatastoreUpdateCommandBuilder getIconDatastoreCommandBuilder(){
		return new IconDatastoreUpdateCommandBuilder();
	}
	
	public IconClientUpdateCommandBuilder getIconClientCommandBuilder(){
		return new IconClientUpdateCommandBuilder();
	}
}
