package com.ibm.ims.ea.om.db.update.syntax;


public class DB_ACCTYPE{
	
	private String result = null;
	
	public enum _DB_ACCTYPE{
		/** The database is available for read-only processing on this IMS subsystem */
		BRWS,
		/** The database is to be used exclusively by this IMS subsystem. */
		EXCL,
		/** The database is available for read-only processing in this IMS subsystem */
		READ,
		/** The database is for update as well as read processing in the IMS subsystem. */
		UPD;
	}

