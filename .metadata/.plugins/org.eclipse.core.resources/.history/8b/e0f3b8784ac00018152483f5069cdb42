package com.ibm.ims.ea.om.icon.query.odbm;

public interface IconOdbmQueryCommand {
	
	public enum SHOW {
		ALL,
		ALIAS,
		RRS,
		STATUS;
		
	}
	
	
	
	public enum STATUS {
		REGISTERED,  
        NOTACTIVE;
	}
	public enum ASTATUS {
		ACTIVE,  
        NOTACTIVE;
	}
	
	public void setName(String[] names);
	public void setAlias(String[] names);
	
	public void setShow(SHOW[] shows);
	
	public void setAStatus(ASTATUS[] astatus);
	
	public void setStatus(STATUS[] status);
	
	public void setRoute(String[] routeName);	
}
