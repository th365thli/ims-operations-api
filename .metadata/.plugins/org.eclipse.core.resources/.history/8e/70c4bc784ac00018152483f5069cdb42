package com.ibm.ims.ea.om.db.update.syntax;

/** Specifies the attributes to be stopped */
public class DB_STOP{
	
	/** Specifies the attributes to be stopped */
	public enum _DB_STOP {
		
		/** Starts offline processing of the database */
		ACCESS,
		
		/** Stops or prevents subsequently scheduled programs from accessing the database, without affecting currently scheduled programs.  */
		SCHD,
		
		/**Stops or prevents transactions or programs from updating the specified DL/I database.  */
		UPDATES;
		
		/** Specifies that the databases named on the command should be made available again by releasing the quiesce on the databases. */

		//QUIESCE
	}
}
