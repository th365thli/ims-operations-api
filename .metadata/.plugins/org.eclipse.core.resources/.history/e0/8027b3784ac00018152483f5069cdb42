package com.ibm.ims.ea.om.pgm.update.syntax;

/** Specifies the resident option. */
public class RESIDENT implements SET{
	
	private String result = null;
	private static String NAME = "RESIDENT";
	
	public enum _PGM_RESIDENT{
		/** The PSB associated with the program is made to be not resident in storage. */
		N,
		
		/** The PSB associated with the program is made resident in storage at IMS cold start or restart. */
		Y;
	}

	public RESIDENT(_PGM_RESIDENT key) {
    	this.result = key.name();
	}
	
	@Override
	public String toString(){
		StringBuilder strBld = new StringBuilder();
		strBld.append(this.result);
	    return strBld.toString();
	}
	
	@Override
	public String getName() {
		return NAME;
	}
}
