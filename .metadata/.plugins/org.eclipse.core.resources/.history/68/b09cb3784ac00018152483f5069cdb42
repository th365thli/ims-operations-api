package com.ibm.ims.ea.om.pgm.update.syntax;

/**
 * Specifies the attribute values to be changed. A program must have 
 * the same characteristics in all systems where it is defined when it is shared
 */
public interface SET {
	public String toString();
	
	public String getName();
}
