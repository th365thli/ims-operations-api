package com.ibm.ims.ea.om.icon;

public class ImsconnectRemote {

    /**
     * Statuses returned from IMSConnect(RMTIMSCON) Attribute SCSTT,STT
     */
    public static enum STATUS {
        
        //Status from Attribute SCSTT:
        CONN("CONN"),
        RECV("RECV"),
        
        //Status from Attribute STT:
        ACTIVE("ACTIVE"),
        NOTACTIVE("NOTACTIVE"),
        STOPPED("STOPPED"),
        RETRYCONN("RETRYCONN");
        
        private String value = "";

        private STATUS(String val){
            this.value = val;
        }

        private STATUS(){}

        @Override
        public String toString(){
            return this.value;
        }
    }
}
