package com.ibm.ims.ea.om.pgm.update.syntax;

/** Specifies the language interface of the program for a GPSB, or defines a DOPT(Y) program as using the Java language.  */
public class LANG implements SET{
	
	private String result = null;
	private static String NAME = "LANG";
	
	public enum _PGM_LANG{
		/** */
		ASSEM,
		
		/** */
		COBOL,
		
		/** */
		JAVA,
		
		/** */
		PASCAL,
		
		/** */
		PLI
		
;
	}

	public LANG(_PGM_LANG key) {
    	this.result = key.name();
	}
	
	@Override
	public String toString(){
		StringBuilder strBld = new StringBuilder();
		strBld.append(this.result);
	    return strBld.toString();
	}
	
	@Override
	public String getName() {
		return NAME;
	}
}
