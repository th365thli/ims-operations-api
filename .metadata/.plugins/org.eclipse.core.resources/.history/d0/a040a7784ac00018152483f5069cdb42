package com.ibm.ims.ea.om.tran.update.syntax;

/** Specifies where IMS should apply the change. The default is ALL. */
public class SCOPE{
	
	/** Specifies where IMS should apply the change. The default is ALL. */
	public enum _SCOPE {
		/** Changes are applied to the active IMS systems to which the command isrouted */
		ALL,
		
		/** Changes are applied to the active IMS systems to which the command is routed. */
		ACTIVE
	}
}
