package com.ibm.ims.ea.om.icon;

public class ImsconnectImsplex {

    /**
     * Statuses returned from IMSConnect(IMSPLEX) Attribute STT
     */
    public static enum STATUS {
        
        CONN("CONN"),
        CONV("CONV"),
        READ("READ"),
        RECV("RECV"),
        WFCM("WFCM"),
        XMIT("XMIT");
        
        private String value = "";

        private STATUS(String val){
            this.value = val;
        }

        private STATUS(){}

        @Override
        public String toString(){
            return this.value;
        }
    }
    
}
