package com.ibm.ims.ea.om.pgm.update.syntax;

/** Specifies whether the program executes in a BMP type region or not.  */
public class BMPTYPE implements SET{
	
	private String result = null;
	private static String NAME = "BMPTYPE";
	
	public enum _PGM_BMPTYPE{
		/** The program does not run in a BMP type region. */
		N,
		
		/** The program runs in a BMP type region. */
		Y;
	}

	public BMPTYPE(_PGM_BMPTYPE key) {
    	this.result = key.name();
	}
	
	@Override
	public String toString(){
		StringBuilder strBld = new StringBuilder();
		strBld.append(this.result);
	    return strBld.toString();
	}
	
	@Override
	public String getName() {
		return NAME;
	}
}
