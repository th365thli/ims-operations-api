package com.ibm.ims.ea.om.db.update.syntax;

public class DB_AREA {
	
private String result = null;
	
	public DB_AREA(String area) {
		this.result = area;
	}
	public enum _DB_AREA{
		AREA;
	}
	
	public String toString() {
		StringBuffer temp = new StringBuffer("AREA(");
		temp.append(result).append(")");
		return temp.toString();
	}

