package com.ibm.ims.ea.om.db.update.syntax;

/** Specifies the attributes to be started. */
public class DB_START {
	
	/** Specifies the attributes to be started. */
	public enum _DB_START {
		
		/** Starts the database and permits access from transactions or programs. */
		ACCESS
		
		/** Specifies that the databases named on the command are to be quiesced to establish a new recovery point */
		//QUIESCE,
	}
}
