package com.ibm.ims.ea.om.icon;

public class ImsconnectDatastore {

    /**
     * Statuses returned from IMSConnect(DATASTORE) Attribute STATE, STT
     */
    public static enum STATUS {
        
        //Status from Attribute STATE:
        AVAIL("AVAIL"),
        WARN("WARN"),
        SEVERE("SEVERE"),
        NA("N/A"),
        
        //Status from Attribute STT:
        ACTIVE("ACTIVE"),
        NOTACTIVE("NOTACTIVE"),
        DISCONNECT("DISCONNECT");
        
        private String value = "";

        private STATUS(String val){
            this.value = val;
        }

        private STATUS(){}

        @Override
        public String toString(){
            return this.value;
        }
    }
}
