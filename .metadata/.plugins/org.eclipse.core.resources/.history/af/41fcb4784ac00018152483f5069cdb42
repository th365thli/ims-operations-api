package com.ibm.ims.ea.om.pgm.update.syntax;

/** Specifies the dynamic option.  */
public class DOPT implements SET{
	
	private String result = null;
	private static String NAME = "DOPT";
	
	public enum _PGM_DOPT{
		/** The PSB associated with this program is not located dynamically. */
		N,
		
		/** The PSB associated with this program is located dynamically. */
		Y;
	}

	public DOPT(_PGM_DOPT key) {
    	this.result = key.name();
	}
	
	@Override
	public String toString(){
		StringBuilder strBld = new StringBuilder();
		strBld.append(this.result);
	    return strBld.toString();
	}
	
	@Override
	public String getName() {
		return NAME;
	}
}
