package com.ibm.ims.ea.om.icon.update.datastore;

public interface IconDatastoreUpdateCommand {
	
	public enum START {
		COMM;
	}
	
	public enum STOP {
		COMM;
	}
	
	public void setName(String[] name);
	
	public void setStart(START comm);
	
	public void setStop(STOP comm);
	
	public void setRoute(String[] imsName); 

}
