package com.ibm.ims.ea.om.tran.update.syntax;


public class CPRI implements SET{
	
	private String result = null;
	
	public enum _CPRI{	// 0 to 14.
		CPRI;
	}

	public CPRI(_CPRI key, int value){
		if(value >=0  && value <=14){
			this.result = Integer.toString(value);
		}else{
	    	throw new IllegalArgumentException("Invalid value argument");//TODO Resource bundle this
	    }
	}
	
	@Override
	public String toString(){
		StringBuilder strBld = new StringBuilder();
		strBld.append(this.result);
	    return strBld.toString();
	}
	
	
}
